{
	"schemaVersion": 1,
	"id": "emojis",
	"version": "${version}",
	"name": "Emojis",
	"description": "Add emojis to Minecraft",
	"authors": [
		"maDU59_"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/emojis/icon.png",
	"environment": "*",
	"accessWidener" : "emojis.aw",
	"entrypoints": {
		"main": [
			"fr.madu59.ExampleMod"
		],
		"client": [
			"fr.madu59.ExampleModClient"
		]
	},
	"mixins": [
		"emojis.mixins.json",
		{
			"config": "emojis.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": ">=1.20.0",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}